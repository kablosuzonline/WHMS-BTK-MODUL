{
    "name": "kablosuzonline/whmcs-btk-reports",
    "description": "WHMCS addon module for BTK (Bilgi Teknolojileri ve İletişim Kurumu) reporting requirements in Turkey.",
    "type": "whmcs-addon",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kablosuz Online",
            "email": "teknik@kablosuzonline.com.tr"
        }
    ],
    "require": {
        "php": ">=8.4.0",
        "ext-soap": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-gd": "*",
        "phpoffice/phpspreadsheet": "^1.29 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "WHMCS\\Module\\Addon\\BtkReports\\App\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}